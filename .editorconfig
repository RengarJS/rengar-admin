[*.{js,jsx,mjs,cjs,ts,tsx,mts,cts,vue}]
[*.{js,jsx,mjs,cjs,ts,tsx,mts,cts,vue}]
# 文件字符集
charset = utf-8
# 缩进大小
indent_size = 2
# 使用空格进行缩进
indent_style = space
# 在文件末尾插入换行符
insert_final_newline = true
# 删除行尾的空白字符
trim_trailing_whitespace = true
# 使用 LF 作为换行符
end_of_line = lf
# 每行的最大长度
max_line_length = 120