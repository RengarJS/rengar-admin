{
  "name": "rengar-admin",
  "version": "1.3.0",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "dev:prod": "vite --mode production",
    "build": "vite build",
    "build:test": "vite build --mode test",
    "preview": "vite preview",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "husky install",
    "postversion": "git push && git push --tags",
    "release": "node scripts/release.js"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@rengar-admin/axios": "workspace:^",
    "@unocss/reset": "66.4.2",
    "@vueuse/core": "^12.8.2",
    "await-to-js": "^3.0.0",
    "axios": "^1.11.0",
    "echarts": "^5.6.0",
    "es-toolkit": "^1.39.10",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "theme-colors": "^0.1.0",
    "vue": "^3.5.18",
    "vue-draggable-plus": "^0.6.0",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.376",
    "@iconify/utils": "^2.3.0",
    "@iconify/vue": "^4.3.0",
    "@rengar-admin/color": "workspace:^",
    "@rengar-admin/unocss": "workspace:^",
    "@rengar-admin/vite-plugin-version": "workspace:^",
    "@rengar-admin/vite-plugin-vue-inject-name": "workspace:^",
    "@rengar-admin/vite-plugin-vue-routes": "workspace:^",
    "@rollup/pluginutils": "^5.2.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.17.2",
    "@unocss/eslint-config": "66.1.0-beta.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "@vue/compiler-sfc": "^3.5.18",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.33.0",
    "eslint-plugin-vue": "^9.33.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "lint-staged": "^15.5.2",
    "naive-ui": "^2.43.1",
    "npm-run-all2": "^7.0.2",
    "only-allow": "^1.2.1",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "unocss": "^66.4.2",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^7.1.3",
    "vite-plugin-html-template-mpa": "^1.0.29",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-tsc": "^2.2.12"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "vue-demi"
    ]
  }
}
